<script type='text/javascript'>
	function simpan()
	{
		var id_jenis_naskah = $("#id_jenis_naskah").val();
		var format_naskah = $("#format_naskah").val();
		
		var r = confirm("Apakah anda yakin akan memperbaharui format <?php echo $get_jenis_naskah->nama_jenis_naskah;?> ?");
		if (r == true) 
		{
			$.ajax({type: "POST", url: "<?php echo base_url();?>C_admin_tahapan_naskah/edit_format_naskah/"
			, data: 
					{
						id_jenis_naskah:id_jenis_naskah
						,format_naskah:format_naskah
					}
			, cache: false
			, success:function(data)
			{
				 //alert(data);
				if(data=='BERHASIL')
				//if(data != false)
				{
					//location.reload();
					alert("Format Berhasil Diperbaharui !");
				}
				else
				{
					alert("TERJADI KESALAHAN JARINGAN/INTERNET");
				}
			} 
			});
		}
	}
	
	
